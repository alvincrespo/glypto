{
  "name": "glypto",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "glypto": "./bin/glypto"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.md\" \"*.json\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "Alvin Crespo",
  "license": "MIT",
  "description": "An experiment using TypeScript to scrape <meta> tags.",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tsconfig/node24": "^24.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0"
  }
}
